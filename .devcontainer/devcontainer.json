{
  "name": "paperwork",
  "context": "..",
  "dockerFile": "./Dockerfile",
  "remoteUser": "developer",
  "mounts": ["source=/dev/bus/usb,target=/dev/bus/usb,type=bind"],
  "settings": {
    "terminal.integrated.defaultProfile.linux": null
      },
  "runArgs": ["--privileged"],
  "extensions": ["dart-code.dart-code", "dart-code.flutter"],
  "workspaceMount": "source=${localWorkspaceFolder}/simple-app,target=/home/developer/simple-app,type=bind,consistency=delegated",
  "workspaceFolder": "/home/developer/simple-app"
}
